<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vaidoos.foodvaidoos">

    <application
        android:name=".AppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.InitialLoginActivity"
            android:theme="@style/AppThemeFullScreen"></activity>
        <activity
            android:name=".activity.StartWizardActivity"
            android:theme="@style/AppThemeFullScreen">

        </activity>
        <activity
            android:name=".activity.LoginRegistrationActivity"
            android:theme="@style/AppThemeFullScreen"></activity>
        <activity
            android:name=".activity.TestOneActivity"
            android:theme="@style/AppThemeFullScreen"></activity>
        <activity
            android:name=".activity.TestTwoActivity"
            android:theme="@style/AppThemeFullScreen" />
        <activity android:name=".activity.HomeActivity"
            android:theme="@style/AppThemeFullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!--Profile Activities-->

        <activity android:name=".activity.profile.ProfileEditActivity"
            android:theme="@style/AppThemeFullScreen">

        </activity>

    </application>

</manifest>